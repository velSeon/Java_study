package com.dao;

import java.util.List;

import org.apache.ibatis.session.SqlSession;



public class OracleMybatisDAO {

	public List<String> select(SqlSession session) {
		//<select id="selectAll"
		List<String>	list=  session.selectList("select");
		//결과가 여러개인 selsect문 실행시 selectList 함수 실행
		//DeptMapper.xml 의 id가 selsectAll인 sql을 수행
		//DeptMapper.xml 의 id="DselectAll"인 sql문 실행
		return list;
	}

}
