<select id="select" parameterType="HashMap" resultType="com.dto.Dept">
<!-- id=selectAll 실행 후 결과를 resultType으로 저장 -->
select pdsubname, pdcost, pdprice from product
<![CDATA[
where pdcost > (select min(pdcost) from product where pdname = #{key1}) and pdcost < (select max(pdcost) from product where pdname = #{key2})
]]>
</select>	
